{
    "name": "v-ice",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "files": [
        "src",
        "dist"
    ],
    "main": "./dist/filters.js",
    "types": "./dist/main.d.ts",
    "module": "./dist/filters.js",
    "source": "./src/main.tsx",
    "exports": {
        ".": {
            "import": "./src/main.tsx"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc --version && tsc --p ./tsconfig.build.json && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest",
        "demo:prod": "npm run build && cd demos/%npm_config_demo% && npm run build && npm run preview",
        "demo": "cd demos/%npm_config_demo% && npm run dev -- --host",
        "analyze": "vite-bundle-visualizer -c vite.config.ts"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.3",
        "acorn": "^8.11.3",
        "blockly": "^11.1.0",
        "buffer": "^6.0.3",
        "events": "^3.3.0",
        "github-markdown-css": "^5.5.1",
        "i18next": "^23.10.1",
        "luxon": "^3.4.4",
        "prettier": "^3.4.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-draggable": "^4.4.6",
        "react-i18next": "^14.1.0",
        "react-markdown": "^9.0.1",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.4.1",
        "remark-directive": "^3.0.0",
        "remark-gfm": "^4.0.0",
        "remark-math": "^6.0.0",
        "reselect": "^5.1.1",
        "terser": "^5.37.0",
        "unist-util-visit": "^5.0.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.25.9",
        "@types/luxon": "^3.4.2",
        "@types/node": "^20.11.24",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.19",
        "concurrently": "^8.2.2",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "postcss": "^8.4.37",
        "rollup-plugin-external-globals": "^0.9.2",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.7",
        "vite": "^6.0.7",
        "vite-bundle-visualizer": "^1.2.1",
        "vite-plugin-dts": "^4.4.0",
        "vite-plugin-node-polyfills": "^0.2.0",
        "vite-plugin-svgr": "^4.3.0",
        "vitest": "^2.1.8"
    },
    "overrides": {
        "vite": "^6.0.7"
    }
}
