{
    "css.customData": [".vscode/tailwind.json"],
    "search.useIgnoreFiles": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/dist/**": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/src/assets/help/**/*.json"
            ],
            "url": "./src/assets/schemas/manual_section_settings.schema.json"
        }
    ]
}
